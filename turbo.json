{
  "$schema": "https://turborepo.org/schema.json",
  "baseBranch": "origin/main",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**/*", "!vendor/**", "mod.js", "mod.js.map"]
    },
    "build:ci": {
      "dependsOn": ["^build:ci"],
      "outputs": ["dist/**/*", "!vendor/**", "mod.js", "mod.js.map"]
    },
    "dev": {
      "cache": false
    },
    "test": {
      "outputs": [],
      "dependsOn": ["$RUNNER_OS", "$NODE_VERSION"]
    },
    "benchmark": {
      "dependsOn": ["^build"],
      "outputs": []
    },
    "lint": {
      "outputs": []
    }
  }
}
